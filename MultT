import java.util.Scanner;

public class MultT {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        System.out.println("Input a number : ");
        int num = scanner.nextInt();

        for (int i = 1; i <= 9; i++) {
            int result = i * num;
            System.out.println(num + " * " + i + " = " + result);
        }
    }
} 
